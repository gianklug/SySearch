{
    "name": "SySearch",
    "description": "Search inside multiple MediaWiki instances",
    "version": "0.7.1",
    "manifest_version": 2,
    "permissions": ["storage", "*://*/*"],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "SySearch",
        "default_icon": "images/sysearch_48.png"
    },
    "icons": {
        "48": "images/sysearch_48.png"
    }
}
